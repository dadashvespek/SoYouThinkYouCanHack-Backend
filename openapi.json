{
  "openapi": "3.0.1",
  "info": {
    "title": "User Schedule Data Plugin",
    "description": "A plugin that allows fetching and filtering of user schedule data.",
    "version": "v1"
  },
  "servers": [
    {
      "url": "https://scary-fashion-eel.cyclic.app/"
    }
  ],
  "paths": {
    "/data/{user_email}?{params}": {
      "get": {
        "operationId": "getScheduleByEmail",
        "summary": "Fetch and filter user schedule data",
        "parameters": [
          {
            "in": "path",
            "name": "user_email",
            "schema": {
              "type": "string"
            },
            "required": true,
            "description": "The email of the user."
          },
          {
            "in": "query",
            "name": "start_datetime",
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "required": true,
            "description": "The start datetime for filtering the data."
          },
          {
            "in": "query",
            "name": "end_datetime",
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "required": true,
            "description": "The end datetime for filtering the data."
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/getUserDataResponse"
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/errorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "getUserDataResponse": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "user_id": {
              "type": "string",
              "description": "The ID of the user."
            },
            "start_datetime": {
              "type": "string",
              "format": "date-time",
              "description": "The start datetime of the schedule entry."
            },
            "end_datetime": {
              "type": "string",
              "format": "date-time",
              "description": "The end datetime of the schedule entry."
            }
          }
        }
      },
      "errorResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "description": "The error message."
          }
        }
      }
    }
  }
}
