{
    "openapi": "3.0.1",
    "info": {
      "title": "User Schedule Data Plugin",
      "description": "A plugin that allows fetching and filtering of user schedule data.",
      "version": "v1"
    },
    "servers": [
      {
        "url": "https://your-app-url.example.com"
      }
    ],
    "paths": {
      "/data/{user_id}": {
        "get": {
          "operationId": "getSchedule",
          "summary": "Fetch and filter user data",
          "parameters": [
            {
              "in": "path",
              "name": "user_id",
              "schema": {
                "type": "string"
              },
              "required": true,
              "description": "The ID of the user."
            },
            {
              "in": "query",
              "name": "start_datetime",
              "schema": {
                "type": "string",
                "format": "date-time"
              },
              "required": false,
              "description": "The start datetime for filtering the data."
            },
            {
              "in": "query",
              "name": "end_datetime",
              "schema": {
                "type": "string",
                "format": "date-time"
              },
              "required": false,
              "description": "The end datetime for filtering the data."
            },
            {
              "in": "query",
              "name": "location",
              "schema": {
                "type": "string"
              },
              "required": false,
              "description": "The location for filtering the data."
            }
          ],
          "responses": {
            "200": {
              "description": "OK",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/getUserDataResponse"
                  }
                }
              }
            },
            "500": {
              "description": "Error",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/errorResponse"
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "getUserDataResponse": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "user_id": {
                "type": "string",
                "description": "The ID of the user."
              },
              "start_datetime": {
                "type": "string",
                "format": "date-time YYYY/MM/DD HH:MM:SS (ISO 8601)",
                "description": "The start datetime of the schedule entry."
              },
              "end_datetime": {
                "type": "string",
                "format": "date-time YYYY/MM/DD HH:MM:SS (ISO 8601)",
                "description": "The end datetime of the schedule entry."
              },
              "location": {
                "type": "string",
                "description": "The location of the schedule entry."
              },
              "event_name": {
                "type": "string",
                "description": "The name of the event in the schedule entry."
              },
              "repeating": {
                "type": "string",
                "description": "The repeating status of the schedule entry."
              }
            }
          }
        },
        "errorResponse": {
          "type": "object",
          "properties": {
            "error": {
              "type": "string",
              "description": "The error message."
            }
          }
        }
      }
    }
  }
  