              "required": true,
              "description": "The id of the workout plan to update",
              "schema": {
                "type": "string"
              }
            }
          ],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "workoutName": {
                      "type": "string"
                    }
                  },
                  "example": {
                    "workoutName": "New Workout Plan"
                  }
                }
              }
            }
          },
          "responses": {
