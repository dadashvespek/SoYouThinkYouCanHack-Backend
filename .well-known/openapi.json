          }
        }
      },
      "/addExercise": {
        "post": {
          "description": "This route adds a new exercise to the Exercise table",
          "tags": ["Exercises"],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "exerciseName": {
                      "type": "string"
                    },
                    "reps": {
                      "type": "number"
                    },
                    "sets": {
                      "type": "number"
                    }
                  },
                  "example": {
                    "exerciseName": "Squats",
                    "reps": 10,
                    "sets": 3
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful operation"
            },
            "400": {
              "description": "Error occurred"
            }
          }
        }
      },
