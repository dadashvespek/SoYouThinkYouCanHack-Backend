              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "exerciseName": {
                      "type": "string"
                    },
                    "reps": {
                      "type": "number"
                    },
                    "sets": {
                      "type": "number"
                    }
                  },
                  "example": {
                    "exerciseName": "Squats",
                    "reps": 12,
                    "sets": 4
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful operation"
            },
            "400": {
              "description": "Error occurred"
            }
          }
        }
      },
      "/deleteExercise/{exerciseid}": {
        "delete": {
          "description": "This route deletes a specific exercise",
          "tags": ["Exercises"],
          "parameters": [
            {
              "in": "path",
