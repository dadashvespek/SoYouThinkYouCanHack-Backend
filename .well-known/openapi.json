"components": {
    "schemas": {
        "User": {
            "type": "object",
            "properties": {
                "userID": {"type": "string"},
                "name": {"type": "string"},
                "email": {"type": "string"},
                "password": {"type": "string"}
            },
            "required": ["userID", "name", "email", "password"]
        },
        "Schedule": {
            "type": "object",
            "properties": {
                "scheduleID": {"type": "string"},
                "userID": {"type": "string"},
                "title": {"type": "string"},
                "startDateTime": {"type": "string", "format": "date-time"},
                "endDateTime": {"type": "string", "format": "date-time"},
                "location": {"type": "string"},
                "description": {"type": "string"}
            },
            "required": ["scheduleID", "userID", "title", "startDateTime", "endDateTime", "location", "description"]
        },
        "Category": {
            "type": "object",
            "properties": {
                "categoryID": {"type": "string"},
                "categoryName": {"type": "string"},
                "description": {"type": "string"}
            },
            "required": ["categoryID", "categoryName", "description"]
        },
        "WorkoutPlan": {
            "type": "object",
            "properties": {
                "workoutID": {"type": "string"},
                "userID": {"type": "string"},
                "workoutName": {"type": "string"}
            },
            "required": ["workoutID", "userID", "workoutName"]
        },
        "Exercise": {
            "type": "object",
            "properties": {
                "exerciseID": {"type": "string"},
                "exerciseName": {"type": "string"},
                "reps": {"type": "integer"},
                "sets": {"type": "integer"}
            },
            "required": ["exerciseID", "exerciseName", "reps", "sets"]
        },
        "WorkoutPlanExercises": {
            "type": "object",
            "properties": {
                "workoutID": {"type": "string"},
                "exerciseID": {"type": "string"}
            },
            "required": ["workoutID", "exerciseID"]
        },
        "WorkoutSchedule": {
            "type": "object",
            "properties": {
                "scheduleID": {"type": "string"},
                "workoutID": {"type": "string"}
            },
            "required": ["scheduleID", "workoutID"]
        },
        "WorkoutCompletion": {
            "type": "object",
            "properties": {
                "userID": {"type": "string"},
                "workoutID": {"type": "string"},
                "completionDate": {"type": "string", "format": "date"},
                "completionTime": {"type": "string", "format": "time"},
                "workoutDone": {"type": "boolean"}
            },
            "required": ["userID", "workoutID", "completionDate", "completionTime", "workoutDone"]
            }
        }
    }
}